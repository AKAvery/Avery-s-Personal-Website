{
    "builds": [
        { "src": "api/index.py", "use": "@vercel/python" }
    ],
    "routes": [
        { "handle": "filesystem" },
        { "src": "/(.*)", "dest": "/api/index.py" }
    ],
    
    "rewrites": [
      { "source": "/kneeEd", "destination": "/kneeEd.html" },
      { "source": "/cankiosk", "destination": "/cankiosk.html" },
      { "source": "/fistbump", "destination": "/fistbump.html" }
    ]
  }
  